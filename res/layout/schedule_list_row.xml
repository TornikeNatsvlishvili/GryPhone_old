<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/event_student_row"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/news_list_background_selector"
    android:padding="2dp" >

    <!-- android:layout_height="?android:attr/listPreferredItemHeight" -->

    <TextView
        android:id="@+id/schedule_course_title"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="center"
        android:paddingBottom="1dp"
        android:text="Course Title"
        android:textColor="@drawable/schedule_list_text_color_selector"
        android:textSize="20sp"
        android:textStyle="bold" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_below="@id/schedule_course_title"
        android:background="@color/white" />

    <!-- android:background="#10ff0000" -->

    <RelativeLayout
        android:id="@+id/schedule_layout_1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/schedule_course_title"
        android:padding="3dp" >

        <TextView
            android:id="@+id/schedule_temp1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/schedule_course_title"
            android:text="Class Time: "
            android:textColor="@drawable/schedule_list_text_color_selector"
            android:textSize="12sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/schedule_class_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/schedule_course_title"
            android:layout_toRightOf="@id/schedule_temp1"
            android:text=" - "
            android:textColor="@drawable/schedule_list_text_color_selector"
            android:textSize="12sp" />

        <LinearLayout
            android:id="@+id/schedule_layout1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/schedule_class_time"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/schedule_temp2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Class Days: "
                android:textColor="@drawable/schedule_list_text_color_selector"
                android:textSize="12sp"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/schedule_class_img_mon"
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_marginLeft="2dp"
                android:background="@drawable/schedule_red_orb" />

            <ImageView
                android:id="@+id/schedule_class_img_tue"
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_marginLeft="2dp"
                android:background="@drawable/schedule_red_orb" />

            <ImageView
                android:id="@+id/schedule_class_img_wed"
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_marginLeft="2dp"
                android:background="@drawable/schedule_red_orb" />

            <ImageView
                android:id="@+id/schedule_class_img_thu"
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_marginLeft="2dp"
                android:background="@drawable/schedule_red_orb" />

            <ImageView
                android:id="@+id/schedule_class_img_fri"
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_marginLeft="2dp"
                android:background="@drawable/schedule_red_orb" />
        </LinearLayout>

        <TextView
            android:id="@+id/schedule_temp4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toLeftOf="@+id/schedule_class_location"
            android:text="Class Location: "
            android:textColor="@drawable/schedule_list_text_color_selector"
            android:textSize="12sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/schedule_class_location"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/schedule_layout1"
            android:layout_alignParentRight="true"
            android:text=" - "
            android:textColor="@drawable/schedule_list_text_color_selector"
            android:textSize="12sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/schedule_temp4"
            android:layout_toLeftOf="@+id/schedule_class_location_room"
            android:text="Class Room: "
            android:textColor="@drawable/schedule_list_text_color_selector"
            android:textSize="12sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/schedule_class_location_room"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/schedule_class_location"
            android:paddingTop="2dp"
            android:text=" - "
            android:textColor="@drawable/schedule_list_text_color_selector"
            android:textSize="12sp" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_below="@id/schedule_layout_1"
        android:background="@color/white" />

    <!-- android:background="#100000ff" -->

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/schedule_layout_1"
        android:padding="3dp" >

        <TextView
            android:id="@+id/schedule_temp3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="Lab Time: "
            android:textColor="@drawable/schedule_list_text_color_selector"
            android:textSize="12sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/schedule_lab_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/schedule_temp3"
            android:text=" - "
            android:textColor="@drawable/schedule_list_text_color_selector"
            android:textSize="12sp" />

        <LinearLayout
            android:id="@+id/schedule_layout2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/schedule_lab_time"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/schedule_temp5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Lab Days: "
                android:textColor="@drawable/schedule_list_text_color_selector"
                android:textSize="12sp"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/schedule_lab_img_mon"
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_marginLeft="2dp"
                android:background="@drawable/schedule_red_orb" />

            <ImageView
                android:id="@+id/schedule_lab_img_tue"
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_marginLeft="2dp"
                android:background="@drawable/schedule_red_orb" />

            <ImageView
                android:id="@+id/schedule_lab_img_wed"
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_marginLeft="2dp"
                android:background="@drawable/schedule_red_orb" />

            <ImageView
                android:id="@+id/schedule_lab_img_thu"
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_marginLeft="2dp"
                android:background="@drawable/schedule_red_orb" />

            <ImageView
                android:id="@+id/schedule_lab_img_fri"
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_marginLeft="2dp"
                android:background="@drawable/schedule_red_orb" />
        </LinearLayout>

        <TextView
            android:id="@+id/schedule_temp6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/schedule_layout2"
            android:layout_toLeftOf="@+id/schedule_lab_location"
            android:text="Lab Location: "
            android:textColor="@drawable/schedule_list_text_color_selector"
            android:textSize="12sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/schedule_lab_location"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/schedule_layout2"
            android:layout_alignParentRight="true"
            android:text=" - "
            android:textColor="@drawable/schedule_list_text_color_selector"
            android:textSize="12sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/schedule_temp6"
            android:layout_toLeftOf="@+id/schedule_lab_location_room"
            android:text="Lab Room: "
            android:textColor="@drawable/schedule_list_text_color_selector"
            android:textSize="12sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/schedule_lab_location_room"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/schedule_lab_location"
            android:paddingTop="2dp"
            android:text=" - "
            android:textColor="@drawable/schedule_list_text_color_selector"
            android:textSize="12sp" />
    </RelativeLayout>

</RelativeLayout>